@model TeamManager.Models.UserList
<div>
    <fieldset>
    <legend>UserAlert Information</legend>
        <div role="form" class="form-horizontal"> 
<div class="form-group">
    <label class="col-sm-2 control-label"> ID</label>
    <div class="col-sm-10 form-control-static">
        @Html.DisplayFor(model => model.user.AUID,new { @class="form-control"})
    </div>
</div>
            
 <div class="form-group">
    <label class="col-sm-2 control-label"> UserID</label>
    <div class="col-sm-10 form-control-static">
        @*@Html.DisplayFor(model => model.user.UserID,new { @class="form-control"})*@
        @Html.Raw(Model.user.UserID)
    </div>
    </div>

<div class="form-group">
    <label class="col-sm-2 control-label"> CcUser</label>
    <div class="col-sm-10 form-control-static">
        @*@Html.DisplayFor(model => model.user.UserID,new { @class="form-control"})*@
        @Html.Raw(Model.user.CcUser)
    </div>
    </div>

<div class="form-group">
    <label class="col-sm-2 control-label"> AlertID</label>
    <div class="col-sm-10 form-control-static">
            @Html.DisplayFor(model => model.user.ABID,new { @class="form-control"})
    </div>
</div>
  
<div class="form-group">
    <label class="col-sm-2 control-label"> Title</label>
    <div class="col-sm-10 form-control-static">
            @Html.DisplayFor(model => model.user.AUTitle,new { @class="form-control"})
    </div>
</div>      
        
  <div class="form-group">
    <label class="col-sm-2 control-label"> Content</label>
    <div class="col-sm-10 form-control-static">
            @Html.Raw(Model.user.AUContent)
    </div>
</div>       

<div class="form-group">
    <label class="col-sm-2 control-label"> Type</label>
    <div class="col-sm-10 form-control-static">
            @Html.DisplayFor(model => model.user.AUType,new { @class="form-control"})
    </div>
</div>  
  
<div class="form-group">
    <label class="col-sm-2 control-label"> Status</label>
    <div class="col-sm-10 form-control-static">
            @Html.DisplayFor(model => model.user.AUStatus,new { @class="form-control"})
    </div>
</div>       
        
<div class="form-group">
    <label class="col-sm-2 control-label"> Interval</label>
    <div class="col-sm-10 form-control-static">
            @Html.DisplayFor(model => model.user.AUInterval,new { @class="form-control"})
    </div>
</div>  
       
<div class="form-group">
    <label class="col-sm-2 control-label"> UnitNum</label>
    <div class="col-sm-10 form-control-static">
        @Html.DisplayFor(model => model.user.AUUnitNum,new { @class="form-control"})
    </div>
</div>

<div class="form-group">
    <label class="col-sm-2 control-label"> Time</label>
    <div class="col-sm-10 form-control-static">
        @Html.DisplayFor(model => model.user.AUTime,new { @class="form-control"})
    </div>
</div>

  <div class="form-group">
    <label class="col-sm-2 control-label"> Remark</label>
    <div class="col-sm-10 form-control-static">
        @Html.DisplayFor(model => model.user.AURemark,new { @class="form-control"})
    </div>
</div>

<div class="form-group">
    <label class="col-sm-2 control-label"> CreateBy</label>
    <div class="col-sm-10 form-control-static">
        @Html.DisplayFor(model => model.user.CreateBy,new { @class="form-control"})
    </div>
</div>

<div class="form-group">
    <label class="col-sm-2 control-label"> CreateDate</label>
    <div class="col-sm-10 form-control-static">
        @Html.DisplayFor(model => model.user.CreateDate,new { @class="form-control"})
    </div>
</div>

<div class="form-group">
    <label class="col-sm-2 control-label"> ModifyBy</label>
    <div class="col-sm-10 form-control-static">
        @Html.DisplayFor(model => model.user.ModifyBy,new { @class="form-control"})
    </div>
</div>

<div class="form-group">
    <label class="col-sm-2 control-label"> ModifyDate</label>
    <div class="col-sm-10 form-control-static">
        @Html.DisplayFor(model => model.user.ModifyDate,new { @class="form-control"})
    </div>
</div> 
    </div>     
    </fieldset>
</div>

 @if (Model.userAttachs.Count()!=0)
{
    <h2>Attachs Information</h2>
    <table class="table table-bordered">
        <tr>
            <th>ID </th>
            <th>Name</th>
            <th>Type</th>
            <th>Status</th>
            <th>Remark</th>
            <th>CreateBy</th>
            <th>CreateDate</th>
            <th>ModifyBy</th>
            <th>ModifyDate</th>
        </tr>
        @foreach (var item in Model.userAttachs) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AUID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AUAName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AUAType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AUAStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AUARemark)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifyBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifyDate)
            </td>
        </tr>
        }
</table>
    }    


   